    theta = x_data(3,1);
    R = [cos(theta) -sin(theta);
         sin(theta) cos(theta)];
    % Determine World Frame Pose of Craft Axes
    pos = x_data(1:2,1);
    
    x_arrow = [pos pos+0.10*cos(theta))];
    y_arrow = [pos pos+(bRw*vect_y)];
end